<Window x:Class="HIPA.Windows.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="300" Width="350" ResizeMode="NoResize">
    <Grid>
        <ComboBox x:Name="NormalizationMethodComboBox" Margin="212,25,12,0" VerticalAlignment="Top" SelectionChanged="NormalizationMethodChanged"/>
        <Label Content="Default Normalization" HorizontalAlignment="Right" Margin="0,25,137,0" VerticalAlignment="Top" Width="197"/>

    </Grid>
</Window>
